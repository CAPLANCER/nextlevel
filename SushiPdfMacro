Sub Debug_ShowTypes()
    Dim ws As Worksheet, r As Long, lr As Long
    Set ws = ThisWorkbook.Sheets("NOVEMBER25")
    lr = ws.Cells(ws.Rows.Count, "C").End(xlUp).Row
    Debug.Print "Row", "Value", "TypeName", "Len", "LastCharCode"
    For r = 2 To lr
        Dim v As Variant, s As String, code As Long
        v = ws.Cells(r, "C").Value
        s = CStr(v)
        If Len(s) = 0 Then
            code = 0
        Else
            code = Asc(Mid(s, Len(s), 1))
        End If
        Debug.Print r, "[" & s & "]", TypeName(v), Len(s), code
    Next r
End Sub
